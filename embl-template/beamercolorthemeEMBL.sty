\mode<all>

\definecolor{lightEMBLgreen}{HTML}{88B57C}
\definecolor{darkEMBLgreen}{HTML}{156D6B}
\definecolor{EMBLred}{HTML}{D92B3C}


\setbeamercolor{palette primary}{bg=lightEMBLgreen}
\setbeamercolor{palette secundary}{bg=darkEMBLgreen,fg=white}
\setbeamercolor{palette tertiary}{bg=darkEMBLgreen,fg=white}
\setbeamercolor{titlelike}{bg=darkEMBLgreen,fg=white}
\setbeamercolor{title}{bg=lightEMBLgreen,fg=black}


\setbeamercolor{section number projected}{bg=darkEMBLgreen}
\setbeamercolor{section in toc}{fg=darkEMBLgreen}

\setbeamercolor{item projected}{bg=darkEMBLgreen}
\setbeamercolor{subitem projected}{parent=item projected}

\setbeamercolor{block title}{parent=titlelike}

\setbeamercolor{block body}{parent=normal text,use=block title,bg=block title.bg!25!bg}

\setbeamercolor{alerted text}{fg=EMBLred}

